@{
    ViewBag.Title = "GetLocation";
}
<h2>Your Sleep Score is @ViewBag.Score</h2>

@ViewBag.Description

@foreach (var item in ViewBag.Images)
{
    <img style="width: 200px; height:auto;" src="@item" />
}

@for (int i = 0; i < ViewBag.Links.Count; i++)
{
    <a href="@ViewBag.Links[i].">@ViewBag.Names[i]</a>
}